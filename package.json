{
  "name": "react-ipsum",
  "version": "0.2.0",
  "description": "React lorem ipsum components",
  "main": "dist",
  "standard": {
    "ignore": [
      "node_modules/**"
    ],
    "parser": "babel-eslint",
    "envs": [
      "jest",
      "node"
    ]
  },
  "scripts": {
    "prebuild": "babel -d lib src",
    "build": "parcel build lib/index.js",
    "storybook": "start-storybook -p 6006 -c .storybook",
    "storybook-deploy": "storybook-to-ghpages",
    "publish": "",
    "test": "jest",
    "lint": "standard 'src/**/*.js!(*.snap)'",
    "lint-fix": "standard 'src/**/*.js!(*.snap)' --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CodeDraken/react-ipsum.git"
  },
  "keywords": [
    "react",
    "lorem-ipsum",
    "components"
  ],
  "author": "CodeDraken",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/CodeDraken/react-ipsum/issues"
  },
  "homepage": "https://github.com/CodeDraken/react-ipsum#readme",
  "peerDependencies": {
    "react": "^16.0.0",
    "react-dom": "^16.0.0"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "lorem-ipsum": "^1.0.6",
    "prop-types": "^15.6.2"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.4.11",
    "@storybook/addon-backgrounds": "^3.4.11",
    "@storybook/addon-centered": "^3.4.11",
    "@storybook/addon-events": "^3.4.11",
    "@storybook/addon-info": "^3.4.11",
    "@storybook/addon-knobs": "^3.4.11",
    "@storybook/addon-links": "^3.4.11",
    "@storybook/addon-notes": "^3.4.11",
    "@storybook/addon-options": "^3.4.11",
    "@storybook/addon-storyshots": "^3.4.11",
    "@storybook/addon-storysource": "^3.4.11",
    "@storybook/addons": "^3.4.11",
    "@storybook/cli": "^3.4.11",
    "@storybook/components": "^3.4.11",
    "@storybook/core-events": "^4.0.0-alpha.11",
    "@storybook/react": "^3.4.11",
    "@storybook/storybook-deployer": "^2.3.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.5.0",
    "jest": "^23.6.0",
    "parcel-bundler": "^1.10.0",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-test-renderer": "^16.5.2",
    "standard": "^12.0.1"
  }
